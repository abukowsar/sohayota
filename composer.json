{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "artesaos/seotools": "^0.16.0",
        "barryvdh/laravel-translation-manager": "^0.5.6",
        "fideloper/proxy": "^4.0",
        "laravel-notification-channels/webpush": "^5.0",
        "laravel/framework": "^6.0",
        "laravel/horizon": "^3.3",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^4.2",
        "laravel/tinker": "^1.0",
        "laravel/ui": "^1.0",
        "laravelcollective/html": "^6.0",
        "lavary/laravel-menu": "^1.7",
        "mews/purifier": "^3.1",
        "qoraiche/laravel-mail-editor": "^1.3",
        "rachidlaasri/laravel-installer": "^4.0",
        "spatie/laravel-activitylog": "^3.8",
        "spatie/laravel-medialibrary": "^7.12",
        "spatie/laravel-permission": "^3.0",
        "spatie/laravel-sluggable": "^2.2",
        "thibaud-dauce/laravel-notifications-mattermost": "^1.1",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-buttons": "^4.7",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.3",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
  "platform": {
    "ext-pcntl": "7.3",
    "ext-posix": "7.3"
  }
},
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helper/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
